{
  "name": "Auckland Museum Collections",
  "manifest_version": 2,
  "version": "1.5",
  "description": "Sets the background of new tabs to an image from the collections of Auckland War Memorial Museum TƒÅmaki Paenga Hira.",
  "homepage_url": "https://www.aucklandmuseum.com/discover/collections/beta",
  "icons": {
    "16": "img/16.png",
    "24": "img/24.png",
    "32": "img/32.png",
    "128": "img/128.png",
    "187": "img/187.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "img/16.png",
      "24": "img/24.png",
      "32": "img/32.png",
      "128": "img/128.png"
    },
    "page_action": {
      "default_icon": {
        "16": "img/16.png",
        "24": "img/24.png",
        "32": "img/32.png",
        "128": "img/128.png",
        "187": "img/187.png"
      }
    },
    "default_title": "Auckland Museum Collections"
  },
  "permissions": ["activeTab"],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}
