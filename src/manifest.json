{
  "name": "Auckland Museum Collections (Beta)",
  "manifest_version": 2.2,
  "version": "1.8.1",
  "description": "See incredible Auckland Museum collection items every time you open a new tab.",
  "homepage_url": "https://www.aucklandmuseum.com/discover/collections/beta",
  "icons": {
    "16": "img/16.png",
    "24": "img/24.png",
    "32": "img/32.png",
    "128": "img/128.png",
    "187": "img/187.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "img/16.png",
      "24": "img/24.png",
      "32": "img/32.png",
      "128": "img/128.png"
    },
    "page_action": {
      "default_icon": {
        "16": "img/16.png",
        "24": "img/24.png",
        "32": "img/32.png",
        "128": "img/128.png",
        "187": "img/187.png"
      }
    },
    "default_title": "Auckland Museum Collections Beta"
  },
  "permissions": ["activeTab"],
  "chrome_url_overrides": {
    "newtab": "./index.html"
  }
}
